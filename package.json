{
  "name": "personal-assist",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start-prod": "cross-env NODE_ENV=production ts-node ./server.ts",
    "start-dev": "cross-env NODE_ENV=development ts-node ./server.ts",
    "start": "cp .env dist && cd dist && node ./server.js",
    "test": "cross-env NODE_ENV=testing ts-mocha",
    "coverage": "nyc -r lcov npm run test",
    "build": "tsc",
    "docs": "typedoc --readme ReadMe.rst --excludeExternals --externalPattern **/node_modules/** --ignoreCompilerErrors --name discord-bot --mode file --out public/ ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node-fetch": "^2.5.3",
    "discord.js": "^11.5.1",
    "dotenv": "^8.0.0",
    "enmap": "^5.0.0",
    "graphql": "^14.3.1",
    "graphql-request": "^1.8.2",
    "koa": "^2.7.0",
    "koa-graphql": "^0.8.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "mongoose": "^5.5.12",
    "node-fetch": "^2.6.0",
    "tsc-openweather": "^0.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.0",
    "@types/koa": "^2.0.48",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-mount": "^3.0.1",
    "@types/koa-router": "^7.0.40",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.5",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.12",
    "supertest": "^4.0.2",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
